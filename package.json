{
	"name": "polyunion",
	"version": "0.1.2",
	"description": "A fast and efficient library for merging multiple polygons in a GeoJSON FeatureCollection",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"repository": {
		"url": "https://github.com/juanpujol/polyunion"
	},
	"scripts": {
		"build": "tsup",
		"format": "prettier --write .",
		"lint": "eslint --fix .",
		"test": "vitest"
	},
	"keywords": [
		"geojson",
		"polygons",
		"mutipolygons",
		"merging",
		"spatial",
		"geospatial",
		"rbush",
		"turfjs",
		"performance"
	],
	"author": "juanpujol",
	"license": "MIT",
	"dependencies": {
		"@turf/bbox": "^6.5.0",
		"@turf/boolean-overlap": "^6.5.0",
		"@turf/helpers": "^6.5.0",
		"@turf/intersect": "^6.5.0",
		"@turf/union": "^6.5.0",
		"rbush": "^3.0.1"
	},
	"devDependencies": {
		"@turf/circle": "^6.5.0",
		"@types/node": "^20.11.30",
		"@types/rbush": "^3.0.3",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"prettier": "^3.2.5",
		"tsup": "^8.0.2",
		"typescript": "^5.3.3",
		"vitest": "^1.3.1"
	}
}